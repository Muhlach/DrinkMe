<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/amarilloClaro"
    tools:context="com.drinkme.sdm.myapplication.CrearCuentaActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <LinearLayout
        android:layout_width="585dp"
        android:layout_height="147dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageViewNombre"
                android:layout_width="110dp"
                android:layout_height="42dp"
                app:srcCompat="@drawable/nombre"
                tools:layout_editor_absoluteX="50dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/editTextNombre"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="Nombre"
                android:inputType="textPersonName"
                android:textColor="@color/granateBase"
                android:textColorHint="@color/granateBase"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="25dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="112dp"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/calendario"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="104dp" />

            <EditText
                android:id="@+id/editText7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="Fecha de Nacimiento"
                android:inputType="date"
                android:textColor="@color/granateBase"
                android:textColorHint="@color/granateBase"
                tools:layout_editor_absoluteX="137dp"
                tools:layout_editor_absoluteY="100dp" />

        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="42dp"
        android:layout_height="29dp"
        android:text="kg."
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        tools:layout_editor_absoluteY="169dp"
        tools:layout_editor_absoluteX="309dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="47dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/altura"
        tools:layout_editor_absoluteY="163dp"
        tools:layout_editor_absoluteX="23dp" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="74dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Altura"
        android:inputType="number"
        android:textColor="@color/granateBase"
        android:textColorHint="@color/granateBase"
        tools:layout_editor_absoluteY="170dp"
        tools:layout_editor_absoluteX="79dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="42dp"
        android:layout_height="29dp"
        android:text="cm."
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        tools:layout_editor_absoluteY="183dp"
        tools:layout_editor_absoluteX="161dp" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="67dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Peso"
        android:inputType="number"
        android:textColor="@color/granateBase"
        android:textColorHint="@color/granateBase"
        tools:layout_editor_absoluteY="164dp"
        tools:layout_editor_absoluteX="222dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="88dp"
        android:layout_height="106dp"
        app:srcCompat="@drawable/usuario"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="209dp" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Contraseña"
        android:inputType="textPersonName"
        android:textColor="@color/granateBase"
        android:textColorHint="@color/granateBase"
        tools:layout_editor_absoluteY="308dp"
        tools:layout_editor_absoluteX="137dp" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Nombre de Usuario"
        android:inputType="textPersonName"
        android:textColor="@color/granateBase"
        android:textColorHint="@color/granateBase"
        tools:layout_editor_absoluteY="257dp"
        tools:layout_editor_absoluteX="137dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="37dp"
        android:layout_height="34dp"
        app:srcCompat="@drawable/correo"
        tools:layout_editor_absoluteY="317dp"
        tools:layout_editor_absoluteX="80dp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="57dp"
        android:layout_height="33dp"
        app:srcCompat="@drawable/password"
        tools:layout_editor_absoluteY="423dp"
        tools:layout_editor_absoluteX="70dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="61dp"
        android:layout_height="34dp"
        app:srcCompat="@drawable/password"
        tools:layout_editor_absoluteY="362dp"
        tools:layout_editor_absoluteX="65dp" />

    <EditText
        android:id="@+id/editText14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Repetir contraseña"
        android:inputType="textPassword"
        android:textColor="@color/granateBase"
        android:textColorHint="@color/granateBase"
        tools:layout_editor_absoluteY="414dp"
        tools:layout_editor_absoluteX="100dp" />

    <EditText
        android:id="@+id/editText15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Contraseña"
        android:inputType="textPassword"
        android:textColor="@color/granateBase"
        android:textColorHint="@color/granateBase"
        tools:layout_editor_absoluteY="354dp"
        tools:layout_editor_absoluteX="137dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="0dp"
        android:text="Crear cuenta"
        android:textAllCaps="false"
        tools:layout_editor_absoluteY="453dp"
        tools:layout_editor_absoluteX="139dp" />

</android.support.constraint.ConstraintLayout>
